@namespace BlazorUI.Components.Pagination
@using BlazorUI.Icons.Lucide.Components

<span aria-hidden="true" class="@CssClass">
    <LucideIcon Name="ellipsis" Class="h-4 w-4" />
    <span class="sr-only">More pages</span>
</span>

@code {
    [Parameter]
    public string? Class { get; set; }

    private string CssClass => BlazorUI.Components.Utilities.ClassNames.cn(
        "flex h-10 w-10 items-center justify-center",
        Class
    );
}
